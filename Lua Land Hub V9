-- Lua Land Hub V9 - Dawid's Fluent UI Version with Scrollable Tabs
-- BY: LLH LUA LAND

local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Lua Land Hub V9",
    SubTitle = "BY: LLH LUA LAND",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl,
    Scrollable = true -- ✅ Makes the left tab list scrollable!
})

-- Tabs (Reordered)
local Tabs = {
    Info = Window:AddTab({ Title = "Information", Icon = "info" }),
    Changelog = Window:AddTab({ Title = "Changelogs", Icon = "list" }),
    Games = Window:AddTab({ Title = "Script Games", Icon = "gamepad" }),
    Universal = Window:AddTab({ Title = "Universal Scripts", Icon = "globe" }),
    Admin = Window:AddTab({ Title = "Admin Scripts", Icon = "shield" }),
    Hub = Window:AddTab({ Title = "Script Hub", Icon = "code" }),
    Creations = Window:AddTab({ Title = "Creations", Icon = "sparkles" }),
    Scanner = Window:AddTab({ Title = "Backdoor Scanner", Icon = "search" }),
    Credits = Window:AddTab({ Title = "Credits", Icon = "info" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

-- Scrollable Sections
local Sections = {}
for name, tab in pairs(Tabs) do
    Sections[name] = tab
end

-- Script Button Helper
local function addScript(tabName, title, url)
    Sections[tabName]:AddButton({
        Title = title,
        Callback = function()
            loadstring(game:HttpGet(url, true))()
        end
    })
end

-- Clipboard Button Helper
local function addClipboard(tabName, title, link)
    Sections[tabName]:AddButton({
        Title = title,
        Callback = function()
            setclipboard(link)
            Fluent:Notify({ Title = "Lua Land Hub", Content = "Link Copied To Clipboard!", Duration = 3 })
        end
    })
end

-- ✅ Information Tab
local player = game.Players.LocalPlayer
local name = player.Name
local age = player.AccountAge
local sex = player.Character and player.Character:FindFirstChild("Body Colors") and "Male" or "Female"
local serverCount = #game.Players:GetPlayers()
local fps = math.floor(1 / game:GetService("RunService").RenderStepped:Wait())
local rigType = player.Character and player.Character:FindFirstChildOfClass("Humanoid") and player.Character:FindFirstChildOfClass("Humanoid").RigType.Name or "Unknown"
local apiStatus = typeof(game.HttpGet) == "function" and "Working" or "Not Working"

Sections.Info:AddParagraph({
    Title = "Player Information",
    Content = string.format(
        "Name: %s\nAccount Age: %d days\nSex: %s\nServer: %d players\nFPS: %d\nRig Type: %s\nAPI: %s",
        name, age, sex, serverCount, fps, rigType, apiStatus
    )
})

-- ✅ Changelogs Tab
Sections.Changelog:AddParagraph({
    Title = "Changelogs",
    Content = "[ + ] Added More Features\n[ + ] Fixed Some Bugs\n[ + ] Added Information Tab\n[ + ] UI Changed To Fluent Library"
})

